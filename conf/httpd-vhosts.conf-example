DocumentRoot "C:/"

<Directory "C:/server/">
  Options None
  AllowOverride None
  Order allow,deny
  Allow from all
</Directory>

# localhost
#
<VirtualHost *:80>
  ServerName localhost
    ServerAlias localhost
  VirtualDocumentRoot "D:/Apache/htdocs"
  <Directory "D:/Apache/htdocs/*">
    AllowOverride All
    Options Indexes FollowSymLinks
    Require local
  </Directory>
  
  <FilesMatch \.php$>
	SetHandler fcgid-script
	FCGIWrapper "D:/Apache/PHP/83/php-cgi.exe" .php
	Options +ExecCGI
  </FilesMatch>
</VirtualHost>

